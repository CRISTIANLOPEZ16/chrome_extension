{
    "manifest_version": 3,
    "name": "SPF Security",
    "description" : "Make Safe Transactions",
    "version": "1.0",
    "icons": {
      "128": "icono.png"
    },
    "permissions": [
      "clipboardRead",
      "clipboardWrite",
      "contextMenus",
      "tabs",
      "notifications"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icono.png"
        }
     },
     "web_accessible_resources": [
        {
          "resources": ["consejos.html"],
          "matches": ["<all_urls>"]
        }
      ]
      
  }
  